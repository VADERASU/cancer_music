{
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:svelte/recommended",
        "plugin:svelte/prettier"
    ],
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion":"latest"
    },
    "rules":{
        "no-underscore-dangle": "off",
        "import/no-mutable-exports": "off",
        "import/prefer-default-export": "off"
    }
}
